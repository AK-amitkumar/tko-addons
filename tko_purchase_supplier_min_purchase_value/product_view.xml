<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record id="product_supplierinfo_form_view_inherit" model="ir.ui.view">
            <field name="name">product.supplierinfo.search</field>
            <field name="model">product.supplierinfo</field>
            <field name="inherit_id" ref="product.product_supplierinfo_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='delay']" position="after">
                    <field name="min_purchase_value" widget='monetary'/>
                    <field name="is_flexible"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
